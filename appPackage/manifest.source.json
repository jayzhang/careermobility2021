{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.9/MicrosoftTeams.schema.json",
    "manifestVersion": "1.9",
    "version": "1.0.0",
    "id": "{appid}",
    "packageName": "com.microsoft.teams.extension",
    "developer": {
        "name": "Teams App, Inc.",
        "websiteUrl": "{baseUrl}",
        "privacyUrl": "{baseUrl}/index.html#/privacy",
        "termsOfUseUrl": "{baseUrl}/index.html#/termsofuse"
    },
    "icons": {
        "color": "color.png",
        "outline": "outline.png"
    },
    "name": {
        "short": "Career Mobility 2021",
        "full": "Career Mobility DevDiv China 2021"
    },
    "description": {
        "short": "Short description of Career Mobility DevDiv China 2021.",
        "full": "Full description of careermobility2021."
    },
    "accentColor": "#FFFFFF",
    "bots": [],
    "composeExtensions": [],
    "configurableTabs": [
        {
            "configurationUrl": "{baseUrl}/index.html#/config",
            "canUpdateConfiguration": true,
            "scopes": [
                "team",
                "groupchat"
            ]
        }
    ],
    "staticTabs": [
        {
            "entityId": "index",
            "name": "Career Mobility",
            "contentUrl": "https://microsoftapc.sharepoint.com/_layouts/15/teamslogon.aspx?SPFX=true&dest=/teams/DevDivChina/SitePages/Career-Mobility.aspx",
            "websiteUrl": "https://microsoftapc.sharepoint.com/teams/DevDivChina/SitePages/Career-Mobility.aspx",
            "scopes": [
                "personal","team"
            ]
        },
        {
          "entityId": "jobs",
          "name": "Jobs",
          "contentUrl": "https://microsoftapc.sharepoint.com/_layouts/15/teamslogon.aspx?SPFX=true&dest=/teams/DevDivChina/SitePages/We-are-hiring.aspx",
          "websiteUrl": "https://microsoftapc.sharepoint.com/teams/DevDivChina/SitePages/We-are-hiring.aspx",
          "scopes": [
              "personal","team"
          ]
       },
       {
         "entityId": "team-portfolios",
         "name": "Team Portfolios",
         "contentUrl": "https://microsoftapc.sharepoint.com/_layouts/15/teamslogon.aspx?SPFX=true&dest=/teams/DevDivChina/SitePages/About-DevDiv-China.aspx",
         "websiteUrl": "https://microsoftapc.sharepoint.com/teams/DevDivChina/SitePages/About-DevDiv-China.aspx",
         "scopes": [
             "personal","team"
         ]
      },
      {
         "entityId": "faq",
         "name": "FAQs",
         "contentUrl": "https://microsoftapc.sharepoint.com/_layouts/15/teamslogon.aspx?SPFX=true&dest=/teams/DevDivChina/SitePages/DevDiv-China-Career-Mobility-2021--FAQs.aspx",
         "websiteUrl": "https://microsoftapc.sharepoint.com/teams/DevDivChina/SitePages/DevDiv-China-Career-Mobility-2021--FAQs.aspx",
         "scopes": [
             "personal",
             "team"
         ]
     },
     {
         "entityId": "v-team",
         "name": "Contact Us",
         "contentUrl": "https://microsoftapc.sharepoint.com/_layouts/15/teamslogon.aspx?SPFX=true&dest=/teams/DevDivChina/SitePages/DevDiv-China-2021-Career-Mobility-Team.aspx",
         "websiteUrl": "https://microsoftapc.sharepoint.com/teams/DevDivChina/SitePages/DevDiv-China-2021-Career-Mobility-Team.aspx",
         "scopes": [
             "personal",
             "team"
         ]  
     }
    ],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": [
        "microsoftapc.sharepoint.com",
        "*.login.microsoftonline.com"
    ],
    "webApplicationInfo": {
        "id": "{appClientId}",
        "resource": "{webApplicationInfoResource}"
    }
}